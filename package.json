{
  "name": "electron-react-rag-project-client",
  "version": "1.0.0",
  "description": "",
  "main": "dist/main/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development concurrently -k \"pnpm:dev:vite\" \"pnpm:dev:electron:watch\" \"pnpm:dev:electron:launch\"",
    "dev:vite": "vite --config build/vite.config.ts",
    "dev:electron:watch": "tsc -p tsconfig.json --watch",
    "dev:electron:launch": "wait-on ./dist/main/index.js && electron .",
    "build": "pnpm run build:react && pnpm run build:electron",
    "build:react": "vite build --config build/vite.config.ts",
    "build:electron": "tsc -p tsconfig.json",
    "package": "pnpm run build && electron-builder --config build/electron-builder.config.js",
    "start": "electron ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/canvas-size": "^1.2.2",
    "@types/lodash": "^4.17.16",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.2",
    "electron": "^19.1.9",
    "electron-builder": "^26.0.12",
    "eslint": "^9.26.0",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.13",
    "wait-on": "^8.0.3"
  },
  "dependencies": {
    "@ai-sdk/react": "^1.2.12",
    "@ant-design/icons": "^6.0.0",
    "@ant-design/use-emotion-css": "^1.0.4",
    "@codesandbox/sandpack-react": "^2.20.0",
    "@tldraw/tldraw": "2.0.0-alpha.17",
    "@types/node": "20",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.3",
    "ai": "4.0.9",
    "antd": "^5.25.1",
    "canvas-size": "^2.0.0",
    "cross-env": "^7.0.3",
    "electron-updater": "^6.6.2",
    "lodash": "^4.17.21",
    "nanoid": "^5.1.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.6.0",
    "react-syntax-highlighter": "^15.6.1",
